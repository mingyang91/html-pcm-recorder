{
	"include": ["src/worker/**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"outDir": "es",
		"target": "ES2022",
		"moduleResolution": "Node",
		"module": "ES2015",
		"alwaysStrict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"lib": ["ES2022"],
		"types": ["audioworklet"]
	}
}